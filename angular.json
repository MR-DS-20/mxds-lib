            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/async-ui-feedback/src/test.ts",
            "tsConfig": "projects/async-ui-feedback/tsconfig.spec.json",
            "karmaConfig": "projects/async-ui-feedback/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/async-ui-feedback/tsconfig.lib.json",
              "projects/async-ui-feedback/tsconfig.spec.json"
